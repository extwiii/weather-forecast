{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-console": ["error", { allow: ["warn", "log"] }],
    "no-nested-ternary": 0,
  },
  "env": {
        "mocha": true,
        "node": true,
        "es6": true,
        "browser": true,
  },
  "parser": "babel-eslint",
}
